<html>

  <head>
    <title>
      Dashboard page
    </title>
    <meta charset='utf-8' />

    <link
      rel='apple-touch-icon'
      sizes='180x180'
      href='/fav/apple-touch-icon.png'
    />
    <link
      rel='icon'
      type='image/png'
      sizes='32x32'
      href='/fav/favicon-32x32.png'
    />
    <link
      rel='icon'
      type='image/png'
      sizes='16x16'
      href='/fav/favicon-16x16.png'
    />
    <link rel='manifest' href='/fav/site.webmanifest' />
    <meta
      name='msapplication-TileColor'
      content='#da532c'
    />
    <meta name='theme-color' content='#ffffff' />

    <link href='css/normalize.css' rel='stylesheet' />

    <link href='css/dashboard.css' rel='stylesheet' />

  </head>

  <body>

    <div class='page page--dashboard'>

      <aside class='page__aside aside vertical'>
        <div class='text aside__logo'>
          Sidebar
        </div>
        <nav class='vertical vertical--small'>

          <a
            href='#'
            class='nav-button nav-button--active click'
          >
            <span
              class='icon nav-button__icon'
              style="background-image: url('/svg/nav/home.svg')"
            ></span>
            <span class='text text--light'>Home</span>
          </a>

          <a href='#' class='nav-button click'>
            <span
              class='icon nav-button__icon'
              style="background-image: url('/svg/nav/orders.svg')"
            ></span>
            <span class='text text--light'>Orders</span>
          </a>

          <a href='#' class='nav-button click'>
            <span
              class='icon nav-button__icon'
              style="background-image: url('/svg/nav/products.svg')"
            ></span>
            <span class='text text--light'>Products</span>
          </a>

        </nav>
      </aside>

      <header class='header'>
        <div class='layout header__content text'>
          <div class='header__search relative'>
            <input
              type='text'
              placeholder='Search'
              class='text field field--header'
            />
            <span
              class='icon header__icon-search'
              style="background-image: url('/svg/header/search.svg')"
            ></span>
            <span
              class='icon header__icon-settings icon--button icon--color-blue click'
              style="background-image: url('/svg/header/settings.svg')"
            ></span>
          </div>

          <div class='icon__block'>
            <span
              class='icon header__icon-right icon--button icon--color-yellow click'
              style="background-image: url('/svg/header/wifi.svg')"
            ></span>
            <span
              class='icon header__icon-right icon--button icon--color-light-blue click'
              style="background-image: url('/svg/header/translate.svg')"
            ></span>
            <span
              class='icon header__icon-right icon--button icon--color-yellow click'
              style="background-image: url('/svg/header/bell.svg')"
            ></span>
          </div>
        </div>

      </header>

      <main class='main'>
        <div class='layout'>

          <section class='box analytic__block'>
            {{! 1 }}
            <div
              class='analytic analytic--double analytic--purple'
            >
              <span
                class='icon analytic__button icon--button icon--color-blue click'
                style="background-image: url('/svg/analytic/earning.svg')"
              ></span>

              <div
                class='relative vertical vertical--small'
              >
                <div
                  class='analytic__title text text--light'
                >$500.00</div>
                <div
                  class='analytic__info text text--small text--light-grey'
                >Total earning</div>

                <span
                  class='icon analytic__arrow'
                  style="background-image: url('/svg/analytic/blue-arrow.svg')"
                ></span>
              </div>
            </div>

            {{! 2 }}
            <div
              class='analytic analytic--double analytic--blue'
            >
              <span
                class='icon analytic__button icon--button icon--color-blue click'
                style="background-image: url('/svg/analytic/order.svg')"
              ></span>

              <div
                class='relative vertical vertical--small'
              >
                <div
                  class='analytic__title text text--light'
                >$961</div>
                <div
                  class='analytic__info text text--small text--light-grey'
                >Total order</div>

              </div>
            </div>

            {{! 3 }}
            <div
              class='analytic analytic--blue analytic--small'
            >
              <span
                class='icon analytic__button icon--button icon--color-blue click'
                style="background-image: url('/svg/analytic/order.svg')"
              ></span>

              <div
                class='relative vertical vertical--small'
              >
                <div
                  class='analytic__title__small text text--light'
                >$203k</div>
                <div
                  class='analytic__info text text--small text--light-grey'
                >Total income</div>

              </div>
            </div>

            {{! 4 }}
            <div
              class='analytic analytic--border--light analytic--small'
            >
              <span
                class='icon analytic__button icon--button icon--color-yellow click'
                style="background-image: url('/svg/analytic/shop.svg')"
              ></span>

              <div
                class='relative vertical vertical--small'
              >
                <div
                  class='analytic__title__small text'
                >$203k</div>
                <div
                  class='analytic__info text text--small'
                >Total income</div>

              </div>
            </div>
          </section>

          <section class='details text'>
            <div class='box purchase vertical'>

              <div class='details__title'>
                Purchase list
              </div>

              <hr class='divider' />

              <div class='purchase__customer'>
                <span>ID437661</span>
                <span>AirOn ProCam</span>
                <span>132.32 EUR</span>
                <span>Customer</span>
                <span>May 11, 2023, 14:21</span>
                <span>7/8 USB Type-C</span>
                <span class='text--warning'>Not paid</span>
                <span>robin@com.de</span>
              </div>

              <hr class='divider' />

              <div class='purchase__customer'>
                <span>ID437661</span>
                <span>PHILIPS Essential</span>
                <span>543 USD</span>
                <span>Customer</span>
                <span>May 11, 2023, 14:21</span>
                <span>HD9200/90</span>
                <span class='text--success'>Pay by Stripe</span>
                <span>dima@com.ua</span>
              </div>

            </div>

            <div class='box payment vertical'>

              <div class='details__title'>
                Payment
              </div>

              <hr class='divider' />

              <div class='payment__type'>
                <div class='form_radio'>
                  <input
                    type='radio'
                    name='Payment_type'
                    checked
                  />
                  <label>Credit card</label>
                </div>

                <div class='form_radio'>
                  <input type='radio' name='Payment_type' />
                  <label>Debit card</label>
                </div>

                <div class='form_radio'>
                  <input type='radio' name='Payment_type' />
                  <label>PayPal</label>
                </div>
              </div>

              <div class='card__details'>

                <div class='card__details--left'>
                  <div class='click single--field'>
                    <label>Name on card</label>

                    {{! <div class='field field--select'></div> }}
                    <input
                      type='text'
                      class='field field--select'
                    />
                  </div>

                  <div class='click single--field'>
                    <label>Expiration</label>
                    <input
                      type='text'
                      class='field field--select'
                    />
                  </div>
                </div>

                <div class='card__details--right'>
                  <div class='click single--field'>
                    <label>Credit card number</label>
                    <input
                      type='text'
                      class='field field--select'
                    />
                  </div>

                  <div class='click single--field'>
                    <label>CVV</label>
                    <input
                      type='text'
                      class='field field--select'
                    />
                  </div>
                </div>

              </div>

              <hr class='divider' />

              <div class='payment__select'>
                <div class='relative click single--field'>
                  <label>Country</label>
                  <div class='field field--select'>
                    <span class='text'>Choose...</span>
                  </div>

                  <span
                    class='icon payment__icon-select'
                    style="background-image: url('/svg/analytic/select-arrow.svg')"
                  ></span>

                  <select class='field__select'>
                    <option value='ukraine'>Ukraine</option>
                    <option value='uk'>UK</option>
                    <option value='usa'>USA</option>
                    <option value='jamaica'>Jamaica</option>
                    <option value='japan'>Japan</option>
                  </select>
                </div>

                <div class='relative click single--field'>
                  <label>State</label>
                  <div class='field field--select'>
                    <span class='text'>Choose...</span>
                  </div>

                  <span
                    class='icon payment__icon-select'
                    style="background-image: url('/svg/analytic/select-arrow.svg')"
                  ></span>

                  <select class='field__select'>
                    <option value='kyiv'>Kyiv</option>
                    <option value='lviv'>Lviv</option>
                    <option
                      value='chernivtsi'
                    >Chernivtsi</option>
                    <option
                      value='ternopil'
                    >Ternopil</option>
                    <option value='dnipro'>Dnipro</option>
                  </select>
                </div>

              </div>

              <hr class='divider' />

              <a
                href='#'
                class='nav-button button--checkout click'
              >

                <span class='text text--light'>Continue to
                  checkout</span>
              </a>

            </div>
          </section>

        </div>
      </main>
    </div>

  </body>

</html>